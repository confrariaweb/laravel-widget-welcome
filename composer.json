{
  "name": "confrariaweb/laravel-widget-welcome",
  "version": "dev-master",
  "description": "Pacote widget de boas vindas para Laravel",
  "keywords": [
    "widget",
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "package",
  "authors": [
    {
      "name": "Rafael Zingano",
      "email": "confrariaweb@gmail.com"
    }
  ],
  "require": {
    "confrariaweb/laravel-vendor": "dev-master",
    "confrariaweb/laravel-widget": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ConfrariaWeb\\WidgetWelcome\\": "src",
      "ConfrariaWeb\\WidgetWelcome\\Databases\\Seeds\\": "src/Databases/Seeds"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ConfrariaWeb\\WidgetWelcome\\Providers\\WidgetWelcomeServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
